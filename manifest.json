{
  "manifest_version": 2,
  "name": "Forgot password? I 'member!",
  "description": "An extension that adds Member Berries to all 'Forgot Password' forms",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "images/member-berries.png",
    "images/member.jpg"
  ]
}
